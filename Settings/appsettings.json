{
  "api": {
    "version": 0.1
  },

  "AllowedHosts": "*",

  "Logging": {
    "IncludeScopes": false,
    "PathFormat": "Logs/log-{Date}.txt",
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Hangfire": "Error"
    }
  }
}
