{
  "AllowedHosts": "*",

  "Logging": {
    "IncludeScopes": false,
    "PathFormat": "Logs/log-{Date}.txt",
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Hangfire": "Error"
    }
  },

  "Settings": {
    "api": {
      "version": "1.0.0",
      "latestUpdate":  "10-03-2019"
    },

    "Minio": {
      "NodeUri": "",
      "Port": 0,
      "DefaultIndex": "documents",
      "Username": "none",
      "Password": "none"
    },

    "Elasticsearch": {
      "NodeUri": "",
      "Port": 0,
      "DefaultIndex": "documents",
      "PipelineDescription": "Attachment pipeline for documents",
      "Username": "none",
      "Password": "none"
    }
  }
}
